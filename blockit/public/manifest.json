{
  "name": "Headline Fetcher",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Retrieves headlines relevant to the website you are currently on",
  "content_scripts": [{
    "js": ["content.js" ],
    "matches": ["<all_urls>"]
   }],
  "content_security_policy": "script-src 'self' 'sha256-di1UBhtad0cZsuIzaz+xs8Y/Euf5CnxkrX2V4kn37jI='; object-src 'self'",
  "background": {
    "scripts": ["background.js"]
   },
  "permissions": ["contextMenus", "tabs"],
  "web_accessible_resources" : ["*.html"]
}
